{
  "extends": [
    "../../../../.eslintrc.json"
  ],
  "ignorePatterns": [
    "!**/*"
  ],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
        "no-useless-constructor": "off",
        "class-methods-use-this": "off",
        "no-empty-function": "off",
        "indent": ["error", 2, { "ignoredNodes": ["PropertyDefinition"] }]
      }
    },
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.js",
        "*.jsx"
      ],
      "rules": {}
    }
  ]
}
