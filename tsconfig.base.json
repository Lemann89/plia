{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es2017",
      "dom"
    ],
    "jsx": "preserve",
    "strictNullChecks": false,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "@plia/plia/auth/api": [
        "libs/plia/auth/api/src/index.ts"
      ],
      "@plia/plia/auth/ui": [
        "libs/plia/auth/ui/src/index.ts"
      ],
      "@plia/plia/editor/api": [
        "libs/plia/editor/api/src/index.ts"
      ],
      "@plia/plia/editor/ui": [
        "libs/plia/editor/ui/src/index.ts"
      ],
      "@plia/plia/landing/ui": [
        "libs/plia/landing/ui/src/index.ts"
      ],
      "@plia/plia/network": [
        "libs/plia/network/src/index.ts"
      ],
      "@plia/plia/site": [
        "libs/plia/site/src/index.ts"
      ],
      "@plia/plia/types": [
        "libs/plia/types/src/index.ts"
      ],
      "@plia/plia/uikit": [
        "libs/plia/uikit/src/index.ts"
      ],
      "@plia/plia/user/api": [
        "libs/plia/user/api/src/index.ts"
      ],
      "@plia/plia/user/ui": [
        "libs/plia/user/ui/src/index.ts"
      ],
      "~editor/ui/src/*": [
        "libs/plia/editor/ui/src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
