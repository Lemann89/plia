{
  "singleQuote": true,
  "bracketSpacing": true,
  "printWidth": 100,
  "trailingComma": "all",
  "proseWrap": "never",
  "bracketSameLine": false,
  "jsxSingleQuote": false,
  "parser": "typescript",
  "tabWidth": 2,
  "semi": true,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^node:(.*)$",
    "",
    "^@plia/(.*)$",
    "",
    "^~(.*)$",
    "",
    "^[./](?!.*\\.scss$).*$",
    "",
    "^(.*).scss",
    ""
  ],
  "importOrderCaseInsensitive": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "classProperties",
    "decorators-legacy",
    "jsx",
    "typescript"
  ]
}
