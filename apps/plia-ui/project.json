{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "apps/plia-ui/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nxext/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/plia-ui",
        "frameworkConfigFile": "@nxext/solid/plugins/vite"
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "@nxext/vite:dev",
      "options": {
        "outputPath": "dist/apps/plia-ui",
        "frameworkConfigFile": "@nxext/solid/plugins/vite"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/plia-ui/**/*.{ts,spec.ts,tsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/plia-ui"],
      "options": {
        "jestConfig": "apps/plia-ui/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
